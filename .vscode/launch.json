  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'cdt'",
      "cargo": {
        "args": [
          "build",
          "--bin=cdt",
          "--package=cdt",
          "--features=debug_refcell"
        ],
        "filter": {
          "name": "cdt",
          "kind": "bin"
        }
      },
      "args": ["./models/model_8.glb", "./models/output.obj"], // Add your run arguments here
      "cwd": "${workspaceFolder}"
    }
  ]
}
